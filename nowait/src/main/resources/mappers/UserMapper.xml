<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ezen.nowait.member.mapper.UserMapper">

<insert id="userInsert"> 
insert into user(user_id,user_pw,user_name,user_birth,user_phone,user_email,user_addr,grade) values(#{userId},#{userPw},#{userName},#{userBirth},#{userPhone},#{userEmail},#{userAddr},#{grade})
</insert>

<select id="userGet" resultType="ezen.nowait.member.domain.UserVO"> 
select * from user where user_id = #{userId}
 </select>

<select id="idCheck" resultType="int"> 
select count(*) from user where user_id = #{userId}
 </select>
</mapper>